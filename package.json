    "lodash": "^4.17.5",
    "vue": "^2.5.13",
    "vue-chartjs": "^3.2.1",
    "vue-router": "^3.0.1",
    "vuejs-datepicker": "^0.9.29",
    "font-awesome": "^4.7.0",
    "moment": "^2.20.1",
